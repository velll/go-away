{
  "manifest_version": 2,
  "name": "Go Away",
  "version": "1.0",

  "description": "Removes any page element",

  "permissions": ["menus", "activeTab"],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["goaway.js"]
    }
  ]

}
